setlocal path=/etc/perl,/usr/local/lib/perl/5.12.4,/usr/local/share/perl/5.12.4,/usr/lib/perl5,/usr/share/perl5,/usr/lib/perl/5.12,/usr/share/perl/5.12,/usr/local/lib/site_perl,,
setlocal nopreserveindent
